{
  "name": "centsa",
  "version": "2.16.0",
  "description": "Centsa money managing solution",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "0.17.1",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "file-mysql-session": "nohorjo/file-mysql-session",
    "google-auth-library": "1.5.0",
    "mysql": "2.15.0",
    "otplib": "10.0.1",
    "supports-color": "5.4.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "istanbul": "0.4.5",
    "mocha": "5.0.4",
    "mochawesome": "3.0.2",
    "node-dev": "^3.1.3",
    "nyc": "13.0.0",
    "sinon": "4.1.5"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=debug node-dev --inspect src/index.js",
    "debug": "NODE_ENV=debug node --inspect-brk=9009 src/index.js",
    "test": "DEBUG=centsa:* DB_IP=someip DB_PORT=1234 DB_USER=username DB_PASSWORD=password DB_NAME=testdb nyc mocha test/**/*Test.js --reporter mochawesome",
    "xtest": "DB_IP=someip DB_PORT=1234 DB_USER=username DB_PASSWORD=password DB_NAME=testdb nyc mocha --reporter mochawesome",
    "postinstall": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nohorjo/Centsa.git"
  },
  "author": "Muhammed Haque",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nohorjo/Centsa/issues"
  },
  "homepage": "https://github.com/nohorjo/Centsa#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
